<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.itzixi.mapper.TeacherMapperCustom" >

  <select id="getTeacherById" parameterType="string" resultType="com.itzixi.pojo.Teacher">
    select
      id,
      `name`,
      age,
      sex
    from
      teacher
    where
      id = #{tid}
  </select>

</mapper>